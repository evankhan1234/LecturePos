                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="No."
                    android:textColor="@android:color/white"
                    android:textSize="7dp" />

                <TextView
                    android:layout_weight="3"
                    android:gravity="center"
                    android:text="Product"
                    android:textColor="@android:color/white"
                    android:textSize="7dp" />

                <TextView
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="Qty"
                    android:textColor="@android:color/white"
                    android:textSize="7dp" />

                <TextView
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="Rate"
                    android:textColor="@android:color/white"
                    android:textSize="7dp" />

                <TextView
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="Tax"
                    android:textColor="@android:color/white"
                    android:textSize="7dp" />

                <TextView
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="Amount"
                    android:textColor="@android:color/white"
                    android:textSize="7dp" />
            </TableRow>

            <TableRow
