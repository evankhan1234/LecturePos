                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="a 5486-4604"
                    android:textColor="@android:color/black"
                    android:textSize="30px" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="jack@brandonic.com"
                    android:textColor="@android:color/black"
                    android:textSize="30px" />

