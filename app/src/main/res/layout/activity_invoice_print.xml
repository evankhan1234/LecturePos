                    android:gravity="top|center_vertical|center_horizontal|center"
                    android:text="41.00"
                    android:textColor="@android:color/black"
                    android:textSize="25px" />

                <TextView
                    android:layout_height="match_parent"
                    android:layout_weight="1"
