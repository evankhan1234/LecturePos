                        android:layout_height="match_parent"
                        android:gravity="start"
                        android:text="Thanks for your business"
                        android:textSize="25px" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TableLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TableRow android:layout_weight="2">

                            <TextView
                                android:layout_weight="1"
                                android:padding="2dp"
                                android:text="Total"
                                android:textColor="@android:color/black"
                                android:textSize="30px"
                                android:textStyle="bold" />

                            <TextView
                                android:layout_weight="1"
                                android:gravity="end"
                                android:padding="2dp"
                                android:paddingRight="25px"
                                android:text="$8,401.62"
                                android:textColor="@android:color/black"
                                android:textSize="30px"
                                android:textStyle="bold" />
                        </TableRow>

                        <TableRow android:layout_weight="2">

                            <TextView
                                android:layout_weight="1"
                                android:padding="2dp"
                                android:text="Shipping     (+)"
                                android:textColor="@android:color/black"
                                android:textSize="30px" />

                            <TextView
                                android:layout_weight="1"
                                android:gravity="end"
                                android:padding="2dp"
                                android:paddingRight="25px"
                                android:text="$2,000.00"
                                android:textColor="@android:color/black"
                                android:textSize="30px" />
                        </TableRow>

                        <TableRow android:layout_weight="2">

                            <TextView
                                android:layout_weight="1"
                                android:padding="2dp"
                                android:text="Discount      (-)"
                                android:textColor="@android:color/black"
                                android:textSize="30px" />

                            <TextView
                                android:layout_weight="1"
                                android:gravity="end"
                                android:padding="2dp"
                                android:paddingRight="25px"
                                android:text="$500.00"
                                android:textColor="@android:color/black"
                                android:textSize="30px" />
                        </TableRow>

                        <TableRow android:layout_weight="2">

                            <TextView
                                android:layout_weight="1"
                                android:padding="2dp"
                                android:text="Adjustment (-)"
                                android:textColor="@android:color/black"
                                android:textSize="30px" />

                            <TextView
                                android:layout_weight="1"
                                android:gravity="end"
                                android:padding="2dp"
                                android:paddingRight="25px"
                                android:text="$01.62"
                                android:textColor="@android:color/black"
                                android:textSize="30px" />
                        </TableRow>

                        <TableRow android:layout_weight="2">

                            <TextView
                                android:layout_weight="1"
                                android:padding="2dp"
                                android:text="Paid              (-)"
                                android:textColor="@android:color/black"
                                android:textSize="30px" />

                            <TextView
                                android:layout_weight="1"
                                android:gravity="end"
                                android:padding="2dp"
                                android:paddingRight="25px"
                                android:text="$3000.00"
                                android:textColor="@android:color/black"
                                android:textSize="30px" />
                        </TableRow>

                        <TableRow
                            android:id="@+id/temp01_table_row_three"
                            android:layout_weight="2"
                            android:background="#4285f4">

                            <TextView
                                android:layout_weight="1"
                                android:padding="2dp"
                                android:text="Balance"
                                android:textColor="@android:color/white"
                                android:textSize="30px"
                                android:textStyle="bold" />

                            <TextView
                                android:layout_weight="1"
                                android:gravity="end"
                                android:paddingRight="25px"
                                android:text="$6,900.00"
                                android:textColor="@android:color/white"
                                android:textSize="30px"
                                android:textStyle="bold" />
                        </TableRow>
                    </TableLayout>

                    <TextView
